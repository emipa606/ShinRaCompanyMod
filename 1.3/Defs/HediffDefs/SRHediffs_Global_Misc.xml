<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <HediffDef Name="SRaddedPart" Abstract="True">
    <hediffClass>Hediff_Implant</hediffClass>
    <isBad>false</isBad>
    <defaultLabelColor>(0.5, 0.5, 0.9)</defaultLabelColor>
    <addedPartProps>
      <solid>true</solid>
    </addedPartProps>
  </HediffDef>
  <HediffDef Name="addedPartSR" Abstract="True">
    <hediffClass>Hediff_AddedPart</hediffClass>
    <isBad>false</isBad>
    <defaultLabelColor>(255,150,242)</defaultLabelColor>
  </HediffDef>
  <HediffDef ParentName="SRaddedPart">
    <defName>JenovaCellsmodified</defName>
    <label>Genetically Modified with Jenova Cells</label>
    <description>Modifies the subject with Jenova cells, granting he/she with a enhanced body strengh, health and other improvements at the cost of... well some of his humanity, WARNING: 25% chance of death on the process.</description>
    <labelNoun>Genetically Modified with Jenova Cells</labelNoun>
    <comps>
      <li Class="HediffCompProperties_HealPermanentWounds" />
    </comps>
    <addedPartProps>
      <partEfficiency>1.0</partEfficiency>
    </addedPartProps>
    <stages>
      <li>
        <painFactor>-0.2</painFactor>
        <capMods>
          <li>
            <capacity>Manipulation</capacity>
            <offset>0.5</offset>
          </li>
          <li>
            <capacity>Moving</capacity>
            <offset>0.25</offset>
          </li>
          <li>
            <capacity>Talking</capacity>
            <offset>0.3</offset>
          </li>
          <li>
            <capacity>Hearing</capacity>
            <offset>0.5</offset>
          </li>
          <li>
            <capacity>Sight</capacity>
            <offset>0.5</offset>
          </li>
          <li>
            <capacity>BloodFiltration</capacity>
            <offset>0.5</offset>
          </li>
          <li>
            <capacity>BloodPumping</capacity>
            <offset>0.5</offset>
          </li>
          <li>
            <capacity>Breathing</capacity>
            <offset>0.3</offset>
          </li>
          <li>
            <capacity>Consciousness</capacity>
            <offset>0.5</offset>
          </li>
        </capMods>
      </li>
    </stages>
  </HediffDef>
</Defs>