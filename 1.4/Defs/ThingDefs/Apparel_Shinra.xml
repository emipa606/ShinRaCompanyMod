<Defs>
  <!--========================= Shin-Ra Armor helmet ==========================-->
  <ThingDef ParentName="HatBase">
    <defName>shinra_armorHelmet</defName>
    <label>Shin-Ra Helmet</label>
    <description>A thick steel helmet, with tinted visor and an incorporated night vision device, Manufactured by Shin-Ra S.A.</description>
    <techLevel>Industrial</techLevel>
    <recipeMaker>
      <unfinishedThingDef>UnfinishedMetallicTechArmor</unfinishedThingDef>
    </recipeMaker>
    <graphicData>
      <texPath>Things/Helmet/SRMPHelmet</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <statBases>
      <WorkToMake>12000</WorkToMake>
      <MaxHitPoints>140</MaxHitPoints>
      <Mass>1.2</Mass>
      <ArmorRating_Blunt>0.25</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.45</ArmorRating_Sharp>
    </statBases>
    <costStuffCount>70</costStuffCount>
    <costList>
      <ComponentIndustrial>2</ComponentIndustrial>
      <Steel>35</Steel>
    </costList>
    <equippedStatOffsets>
      <ShootingAccuracyPawn>0.10</ShootingAccuracyPawn>
      <MoveSpeed>-0.02</MoveSpeed>
    </equippedStatOffsets>
    <apparel>
      <bodyPartGroups>
        <li>UpperHead</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Helmet/SRMPHelmet</wornGraphicPath>
      <layers>
        <li>Overhead</li>
      </layers>
      <tags>
        <li>Military</li>
        <li>Shinra</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
    <tradeTags>
      <li>Shinraequipment</li>
    </tradeTags>
  </ThingDef>
